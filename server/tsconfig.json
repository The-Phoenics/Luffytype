{
  "compilerOptions": {
    "target": "ESNext",
    // "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}